<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="flow_horizontalSeparator" />
    <attr name="flow_verticalSeparator" />
    <attr name="motionProgress" />
    <attr name="waveDecay" />
    <attr name="motionPathRotate" />
    <attr name="duration" />

    <declare-styleable name="EditTextValid">
        <attr name="etvDrawableValid" format="reference" />
        <attr name="etvDrawableInvalid" format="reference" />
        <attr name="etvBackgroundValid" format="reference" />
        <attr name="etvBackgroundInvalid" format="reference" />
    </declare-styleable>
    <declare-styleable name="PasswordValidEditText">
        <attr name="pvetDrawableValid" format="reference" />
        <attr name="pvetDrawableInvalid" format="reference" />
        <attr name="pvetDrawableVisible" format="reference" />
        <attr name="pvetDrawableInvisible" format="reference" />
        <attr name="pvetBackgroundValid" format="reference" />
        <attr name="pvetBackgroundInvalid" format="reference" />
        <attr name="android:hint" format="string" />
    </declare-styleable>
    <declare-styleable name="ImageTransformView">
        <attr name="itvDrawableStart" format="reference" />
        <attr name="itvDrawableEnd" format="reference" />
        <attr name="itvOffsetStart" format="integer" />
        <attr name="itvOffsetEnd" format="integer" />
    </declare-styleable>
    <attr name="pi_style" format="reference" />

    <attr name="pi_type" format="enum">
        <enum name="line" value="0" />
        <enum name="circle" value="1" />
    </attr>

    <attr name="pi_edgeAnimation" format="flags">
        <flag name="size" value="1" />
        <flag name="alpha" value="2" />
    </attr>

    <declare-styleable name="PagerIndicatorView">
        <attr name="pi_type" />
        <attr name="pi_edgeAnimation" />
        <attr name="pi_size" format="dimension" />
        <attr name="pi_padding" format="dimension" />
        <attr name="pi_length" format="dimension" />
        <attr name="pi_maxVisibleItems" format="integer" />
        <attr name="android:colorBackground" />
        <attr name="android:colorFocusedHighlight" />
    </declare-styleable>
</resources>